{
  "name": "atcoder-review",
  "version": "0.0.1",
  "license": "MIT",
  "author": {
    "name": "jueve,",
    "email": "o8g7no9q@gmail.com",
    "url": "https://github.com/jueve"
  },
  "description": "A cross-platform desktop application for reviewing problems of AtCoder.",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:windows": "yarn compile && electron-builder --win --x64",
    "dist:mac": "yarn compile && electron-builder --mac --x64",
    "dist:linux": "yarn compile && electron-builder --linux --x64",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint --ext .ts,.tsx ./src/**",
    "prettier": "prettier --write ./** --ignore-path .gitignore",
    "icon": "electron-icon-maker --input=./icon/base.png --output=./"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/styles": "^4.10.0",
    "immutable": "^4.0.0-rc.12",
    "knex": "^0.21.5",
    "node-pre-gyp": "^0.15.0",
    "sort-package-json": "^1.44.0",
    "source-map-support": "^0.5.16",
    "sqlite3": "^5.0.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.10.4",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "electron": "8.2.0",
    "electron-builder": "^22.4.1",
    "electron-icon-maker": "^0.0.5",
    "electron-webpack": "^2.8.2",
    "electron-webpack-eslint": "^6.0.0",
    "electron-webpack-ts": "^4.0.1",
    "eslint": "^7.7.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "moment": "^2.27.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "sass-loader": "^9.0.3",
    "start": "^5.1.0",
    "typescript": "^3.9.7",
    "webpack": "~4.42.1"
  },
  "electronWebpack": {
    "main": {
      "webpackConfig": "./custom.webpack.js"
    },
    "renderer": {
      "webpackConfig": "./custom.webpack.js"
    }
  },
  "build": {
    "productName": "Atcoder Review",
    "appId": "atcoder-review.jueve.github",
    "directories": {
      "output": "./dist"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "linux": {
      "icon": "./icons/png"
    },
    "mac": {
      "category": "Productivity",
      "icon": "./icons/mac/icon.icns",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "icon": "./icons/win/icon.ico",
      "target": "msi"
    }
  }
}
